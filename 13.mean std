num_values <- as.integer(readline(prompt = "Enter the number of random numbers to generate: "))
mean_value <- as.numeric(readline(prompt = "Enter the mean of the normal distribution: "))
sd_value <- as.numeric(readline(prompt = "Enter the standard deviation of the normal distribution: "))
random_numbers <- round(rnorm(num_values, mean = mean_value, sd = sd_value), 1)
value_counts <- table(random_numbers)
cat("\nRandom numbers from normal distribution:\n")
print(random_numbers)
cat("\nOccurrences of each unique value:\n")
print(value_counts)
